@import "../../views/game/game.wxss";

.toolbar { display: flex; height : 100rpx; border-top: solid 1px #eee; position: fixed; bottom : 0; width : 100%; background-color: #fff; }
.toolbar .tab { flex-grow : 1; text-align: center; font-size : 24rpx; color : #888; position: relative; }
.toolbar .tab .icon { display: block; margin : 0 auto; margin-top: 10rpx; }
.toolbar .tab.current { color : #f5692e; }